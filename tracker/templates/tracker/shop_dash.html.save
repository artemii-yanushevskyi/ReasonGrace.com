{% load static %}
{% load tz %}
<html>
<head>
  <title>Shop | {{ seller }}</title>
  <link rel="stylesheet" href="{% static 'css/layout.css' %}">
  <script src="{% static 'js/tablify.js' %}"></script>
  <link href="https://fonts.googleapis.com/css?family=Montserrat:100,400" rel="stylesheet">
</head>
<body>


<form method='POST'>
  {% csrf_token %} {{ form }} <br>
  <input type="submit" value="Submit">
</form>

<div id="purchases_table"></div>
<script>
  var purchases = {{ table | safe }};
  document.getElementById("purchases_table").innerHTML = tablifylist(purchases,"Purchases");
</script>

<body>
</html>
